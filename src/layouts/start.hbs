<!doctype html>
<html>
<head>
  <!-- REDIRECT SCRIPT -->
  <script src='{{assets}}/js/redirect.js'></script>
  <meta charset='utf-8'>
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="viewport" content="width=device-width">

  <title>Particle Documentation (formerly Spark) | {{title}}</title>

  <!-- jQuery -->
  <script src="{{assets}}/js/jquery-1.11.1.min.js"></script>

  <!-- Favicon -->
  <link rel="shortcut icon" href="{{assets}}/orange.ico" type="image/x-icon" />

  <!-- Vendor theme mods -->
  <script src='{{assets}}/js/prettify.js'></script>
  <!-- <script src='{{assets}}/js/scrollspy.js'></script> -->

  <!-- Spark theme mods -->
  <link href='{{assets}}/css/style.css' rel='stylesheet'>
  <script src='{{assets}}/js/animated-core.js'></script>

  <!-- GOTHAM YO -->
  <link rel="stylesheet" type="text/css" href="//cloud.typography.com/6107112/665726/css/fonts.css" />

  <!-- Meta -->
  <meta content="Particle" property="og:title">
  <meta content="Documentation for Particle, a platform for connected devices." name="description">


</head>
<body>

  <div id="docs">
    <div class='header'>
      <div class='left'>
        <a href="https://www.particle.io">
          <img src="{{assets}}/images/particle-docs.png" id="logo">
        </a>
      </div>
    </div>

    <div class='getting-started'>
      {{#markdown}}
        {{> body}}
      {{/markdown}}
    </div>

  <!-- Custom -->
  <script type="text/javascript" src="https://s3.amazonaws.com/redirect.particle.io/redirect-banner.js"></script>
  <script src='{{assets}}/js/docs.js'></script>
  <script src='{{assets}}/js/ga.js'></script>
